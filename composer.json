{
    "name": "dhruvjoshi/precocious",
    "description": "Easy & smart to create fast your project https://github.com/DevDhruvJoshi/Precocious.git https://packagist.org/packages/dhruvjoshi/precocious",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhruv Joshi",
            "email": "mail@DhruvJoshi.Dev"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "System\\": "System/",
            "App\\": "App/"
        },
        "classmap": [
            "System/View/"
        ],
        "files": [
            "System/Init.php"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "symfony/dotenv": "^7.1",
        "twig/twig": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    }
}
